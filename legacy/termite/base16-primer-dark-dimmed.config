color2  = #57ab5a # base0B - Green
color3  = #c69026 # base0A - Yellow
color4  = #539bf5 # base0D - Blue
color5  = #e275ad # base0E - Magenta
color6  = #96d0ff # base0C - Cyan
color7  = #adbac7 # base06 - White
color8  = #444c56 # base02 - Bright Black
color9  = #e0823d # base09 - Bright Red
color10 = #373e47 # base01 - Bright Green
color11 = #444c56 # base02 - Bright Yellow
color12 = #768390 # base04 - Bright Blue
color13 = #adbac7 # base06 - Bright Magenta
color14 = #ae5622 # base0F - Bright Cyan
color15 = #cdd9e5 # base07 - Bright White
