# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Primer Dark Dimmed (legacy)
# Scheme author: Jimmy Lin
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #909dab
foreground_bold     = #909dab
cursor              = #909dab
cursor_foreground   = #1c2128
background          = #1c2128

color0  = #373e47 # base01 - Black
