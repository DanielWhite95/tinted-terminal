# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Equilibrium Gray Dark (legacy)
# Scheme author: Carlo Abelli
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #ababab
foreground_bold     = #ababab
cursor              = #ababab
cursor_foreground   = #111111
background          = #111111

color0  = #1b1b1b # base01 - Black
