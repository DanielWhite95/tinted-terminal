color2  = #7f8b00 # base0B - Green
color3  = #bb8801 # base0A - Yellow
color4  = #008dd1 # base0D - Blue
color5  = #6a7fd2 # base0E - Magenta
color6  = #00948b # base0C - Cyan
color7  = #c6c6c6 # base06 - White
color8  = #262626 # base02 - Bright Black
color9  = #df5923 # base09 - Bright Red
color10 = #1b1b1b # base01 - Bright Green
color11 = #262626 # base02 - Bright Yellow
color12 = #919191 # base04 - Bright Blue
color13 = #c6c6c6 # base06 - Bright Magenta
color14 = #e3488e # base0F - Bright Cyan
color15 = #e2e2e2 # base07 - Bright White
