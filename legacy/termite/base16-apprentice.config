color3  = #87af87 # base0A - Yellow
color4  = #8787af # base0D - Blue
color5  = #5fafaf # base0E - Magenta
color6  = #87afd7 # base0C - Cyan
color7  = #5f8787 # base06 - White
color8  = #5f875f # base02 - Bright Black
color9  = #ff8700 # base09 - Bright Red
color10 = #af5f5f # base01 - Bright Green
color11 = #5f875f # base02 - Bright Yellow
color12 = #5f87af # base04 - Bright Blue
color13 = #5f8787 # base06 - Bright Magenta
color14 = #bcbcbc # base0F - Bright Cyan
color15 = #6c6c6c # base07 - Bright White
