color3  = #e5c07b # base0A - Yellow
color4  = #61afef # base0D - Blue
color5  = #d55fde # base0E - Magenta
color6  = #2bbac5 # base0C - Cyan
color7  = #b6bdca # base06 - White
color8  = #2c313a # base02 - Bright Black
color9  = #d19a66 # base09 - Bright Red
color10 = #1c1f24 # base01 - Bright Green
color11 = #2c313a # base02 - Bright Yellow
color12 = #565c64 # base04 - Bright Blue
color13 = #b6bdca # base06 - Bright Magenta
color14 = #be5046 # base0F - Bright Cyan
color15 = #c8ccd4 # base07 - Bright White
