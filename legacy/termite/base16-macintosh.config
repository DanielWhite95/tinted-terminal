# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Macintosh (legacy)
# Scheme author: Rebecca Bettencourt (http://www.kreativekorp.com)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #c0c0c0
foreground_bold     = #c0c0c0
cursor              = #c0c0c0
cursor_foreground   = #000000
background          = #000000

color0  = #404040 # base01 - Black
color1  = #dd0907 # base08 - Red
color2  = #1fb714 # base0B - Green
color3  = #fbf305 # base0A - Yellow
color4  = #0000d3 # base0D - Blue
color5  = #4700a5 # base0E - Magenta
color6  = #02abea # base0C - Cyan
color7  = #c0c0c0 # base06 - White
color8  = #404040 # base02 - Bright Black
color9  = #ff6403 # base09 - Bright Red
color10 = #404040 # base01 - Bright Green
color11 = #404040 # base02 - Bright Yellow
color12 = #808080 # base04 - Bright Blue
color13 = #c0c0c0 # base06 - Bright Magenta
color14 = #90713a # base0F - Bright Cyan
color15 = #ffffff # base07 - Bright White
