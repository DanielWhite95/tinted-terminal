# Termite Terminal Theme
# Scheme system: base16
# Scheme name: pinky (legacy)
# Scheme author: Benjamin (https://github.com/b3nj5m1n)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #f5f5f5
foreground_bold     = #f5f5f5
cursor              = #f5f5f5
cursor_foreground   = #171517
background          = #171517

color0  = #1b181b # base01 - Black
color1  = #ffa600 # base08 - Red
color2  = #ff0066 # base0B - Green
color3  = #20df6c # base0A - Yellow
color4  = #00ffff # base0D - Blue
color5  = #007fff # base0E - Magenta
color6  = #6600ff # base0C - Cyan
color7  = #ffffff # base06 - White
color8  = #1d1b1d # base02 - Bright Black
color9  = #00ff66 # base09 - Bright Red
color10 = #1b181b # base01 - Bright Green
color11 = #1d1b1d # base02 - Bright Yellow
color12 = #e7dbdb # base04 - Bright Blue
color13 = #ffffff # base06 - Bright Magenta
color14 = #df206c # base0F - Bright Cyan
color15 = #f7f3f7 # base07 - Bright White
