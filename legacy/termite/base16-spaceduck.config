color3  = #f2ce00 # base0A - Yellow
color4  = #7a5ccc # base0D - Blue
color5  = #b3a1e6 # base0E - Magenta
color6  = #00a3cc # base0C - Cyan
color7  = #c1c3cc # base06 - White
color8  = #30365f # base02 - Bright Black
color9  = #e39400 # base09 - Bright Red
color10 = #1b1c36 # base01 - Bright Green
color11 = #30365f # base02 - Bright Yellow
color12 = #818596 # base04 - Bright Blue
color13 = #c1c3cc # base06 - Bright Magenta
color14 = #ce6f8f # base0F - Bright Cyan
color15 = #ffffff # base07 - Bright White
