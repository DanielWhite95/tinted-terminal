# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Zenburn (legacy)
# Scheme author: elnawe
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #dcdccc
foreground_bold     = #dcdccc
cursor              = #dcdccc
cursor_foreground   = #383838
background          = #383838

color0  = #404040 # base01 - Black
color1  = #dca3a3 # base08 - Red
color2  = #5f7f5f # base0B - Green
color3  = #e0cf9f # base0A - Yellow
color4  = #7cb8bb # base0D - Blue
color5  = #dc8cc3 # base0E - Magenta
color6  = #93e0e3 # base0C - Cyan
color7  = #c0c0c0 # base06 - White
color8  = #606060 # base02 - Bright Black
color9  = #dfaf8f # base09 - Bright Red
color10 = #404040 # base01 - Bright Green
color11 = #606060 # base02 - Bright Yellow
color12 = #808080 # base04 - Bright Blue
color13 = #c0c0c0 # base06 - Bright Magenta
color14 = #000000 # base0F - Bright Cyan
color15 = #ffffff # base07 - Bright White
