# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Colors (legacy)
# Scheme author: mrmrs (http://clrs.cc)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #bbbbbb
foreground_bold     = #bbbbbb
cursor              = #bbbbbb
cursor_foreground   = #111111
background          = #111111

color0  = #333333 # base01 - Black
color1  = #ff4136 # base08 - Red
color2  = #2ecc40 # base0B - Green
color3  = #ffdc00 # base0A - Yellow
color4  = #0074d9 # base0D - Blue
color5  = #b10dc9 # base0E - Magenta
color6  = #7fdbff # base0C - Cyan
color7  = #dddddd # base06 - White
color8  = #555555 # base02 - Bright Black
color9  = #ff851b # base09 - Bright Red
color10 = #333333 # base01 - Bright Green
color11 = #555555 # base02 - Bright Yellow
color12 = #999999 # base04 - Bright Blue
color13 = #dddddd # base06 - Bright Magenta
color14 = #85144b # base0F - Bright Cyan
color15 = #ffffff # base07 - Bright White
