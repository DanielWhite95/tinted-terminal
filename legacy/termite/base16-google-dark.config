color3  = #fba922 # base0A - Yellow
color4  = #3971ed # base0D - Blue
color5  = #a36ac7 # base0E - Magenta
color6  = #3971ed # base0C - Cyan
color7  = #e0e0e0 # base06 - White
color8  = #373b41 # base02 - Bright Black
color9  = #f96a38 # base09 - Bright Red
color10 = #282a2e # base01 - Bright Green
color11 = #373b41 # base02 - Bright Yellow
color12 = #b4b7b4 # base04 - Bright Blue
color13 = #e0e0e0 # base06 - Bright Magenta
color14 = #3971ed # base0F - Bright Cyan
