color3  = #f3f99d # base0A - Yellow
color4  = #57c7ff # base0D - Blue
color5  = #ff6ac1 # base0E - Magenta
color6  = #9aedfe # base0C - Cyan
color7  = #eff0eb # base06 - White
color8  = #43454f # base02 - Bright Black
color9  = #ff9f43 # base09 - Bright Red
color10 = #34353e # base01 - Bright Green
color11 = #43454f # base02 - Bright Yellow
color12 = #a5a5a9 # base04 - Bright Blue
color13 = #eff0eb # base06 - Bright Magenta
color14 = #b2643c # base0F - Bright Cyan
color15 = #f1f1f0 # base07 - Bright White
