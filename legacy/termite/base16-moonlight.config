# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Moonlight (legacy)
# Scheme author: Jeremy Swinarton (https://github.com/jswinarton)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #a3ace1
foreground_bold     = #a3ace1
cursor              = #a3ace1
cursor_foreground   = #212337
background          = #212337

color0  = #403c64 # base01 - Black
color1  = #ff5370 # base08 - Red
color2  = #2df4c0 # base0B - Green
color3  = #ffc777 # base0A - Yellow
color4  = #40ffff # base0D - Blue
color5  = #b994f1 # base0E - Magenta
color6  = #04d1f9 # base0C - Cyan
color7  = #b4a4f4 # base06 - White
color8  = #596399 # base02 - Bright Black
color9  = #f67f81 # base09 - Bright Red
color10 = #403c64 # base01 - Bright Green
color11 = #596399 # base02 - Bright Yellow
color12 = #a1abe0 # base04 - Bright Blue
color13 = #b4a4f4 # base06 - Bright Magenta
color14 = #ecb2f0 # base0F - Bright Cyan
color15 = #ef43fa # base07 - Bright White
