color3  = #e0ac16 # base0A - Yellow
color4  = #88a4d3 # base0D - Blue
color5  = #bb90e2 # base0E - Magenta
color6  = #6eb958 # base0C - Cyan
color7  = #d7c8bc # base06 - White
color8  = #48413a # base02 - Bright Black
color9  = #ca7f32 # base09 - Bright Red
color10 = #302b25 # base01 - Bright Green
color11 = #48413a # base02 - Bright Yellow
color12 = #b4a490 # base04 - Bright Blue
color13 = #d7c8bc # base06 - Bright Magenta
color14 = #b49368 # base0F - Bright Cyan
color15 = #e4d4c8 # base07 - Bright White
