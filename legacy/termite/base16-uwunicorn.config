color5  = #78a38f # base0E - Magenta
color6  = #9c5fce # base0C - Cyan
color7  = #d9c2c6 # base06 - White
color8  = #46354a # base02 - Bright Black
color9  = #de5b44 # base09 - Bright Red
color10 = #2f2a3f # base01 - Bright Green
color11 = #46354a # base02 - Bright Yellow
color12 = #7e5f83 # base04 - Bright Blue
color13 = #d9c2c6 # base06 - Bright Magenta
color14 = #a3a079 # base0F - Bright Cyan
color15 = #e4ccd0 # base07 - Bright White
