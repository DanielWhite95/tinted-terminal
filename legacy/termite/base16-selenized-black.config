color3  = #dbb32d # base0A - Yellow
color4  = #368aeb # base0D - Blue
color5  = #a580e2 # base0E - Magenta
color6  = #3fc5b7 # base0C - Cyan
color7  = #dedede # base06 - White
color8  = #3b3b3b # base02 - Bright Black
color9  = #e67f43 # base09 - Bright Red
color10 = #252525 # base01 - Bright Green
color11 = #3b3b3b # base02 - Bright Yellow
color12 = #777777 # base04 - Bright Blue
color13 = #dedede # base06 - Bright Magenta
color14 = #eb6eb7 # base0F - Bright Cyan
color15 = #dedede # base07 - Bright White
