color4  = #008dd1 # base0D - Blue
color5  = #6a7fd2 # base0E - Magenta
color6  = #00948b # base0C - Cyan
color7  = #cac6bd # base06 - White
color8  = #22262d # base02 - Bright Black
color9  = #df5923 # base09 - Bright Red
color10 = #181c22 # base01 - Bright Green
color11 = #22262d # base02 - Bright Yellow
color12 = #949088 # base04 - Bright Blue
color13 = #cac6bd # base06 - Bright Magenta
color14 = #e3488e # base0F - Bright Cyan
color15 = #e7e2d9 # base07 - Bright White
