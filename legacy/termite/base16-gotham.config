color7  = #99d1ce # base06 - White
color8  = #091f2e # base02 - Bright Black
color9  = #d26937 # base09 - Bright Red
color10 = #11151c # base01 - Bright Green
color11 = #091f2e # base02 - Bright Yellow
color12 = #245361 # base04 - Bright Blue
color13 = #99d1ce # base06 - Bright Magenta
color14 = #4e5166 # base0F - Bright Cyan
color15 = #d3ebe9 # base07 - Bright White
