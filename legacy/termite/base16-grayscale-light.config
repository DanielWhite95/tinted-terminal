color2  = #8e8e8e # base0B - Green
color3  = #a0a0a0 # base0A - Yellow
color4  = #686868 # base0D - Blue
color5  = #747474 # base0E - Magenta
color6  = #868686 # base0C - Cyan
color7  = #252525 # base06 - White
color8  = #b9b9b9 # base02 - Bright Black
color9  = #999999 # base09 - Bright Red
color10 = #e3e3e3 # base01 - Bright Green
color11 = #b9b9b9 # base02 - Bright Yellow
color12 = #525252 # base04 - Bright Blue
color13 = #252525 # base06 - Bright Magenta
color14 = #5e5e5e # base0F - Bright Cyan
color15 = #101010 # base07 - Bright White
