color2  = #00e756 # base0B - Green
color3  = #fff024 # base0A - Yellow
color4  = #83769c # base0D - Blue
color5  = #ff77a8 # base0E - Magenta
color6  = #29adff # base0C - Cyan
color7  = #c2c3c7 # base06 - White
color8  = #7e2553 # base02 - Bright Black
color9  = #ffa300 # base09 - Bright Red
color10 = #1d2b53 # base01 - Bright Green
color11 = #7e2553 # base02 - Bright Yellow
color12 = #ab5236 # base04 - Bright Blue
color13 = #c2c3c7 # base06 - Bright Magenta
color14 = #ffccaa # base0F - Bright Cyan
color15 = #fff1e8 # base07 - Bright White
