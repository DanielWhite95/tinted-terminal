# Termite Terminal Theme
# Scheme system: base16
# Scheme name: iA Dark (legacy)
# Scheme author: iA Inc. (modified by aramisgithub)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #cccccc
foreground_bold     = #cccccc
cursor              = #cccccc
cursor_foreground   = #1a1a1a
background          = #1a1a1a

color0  = #222222 # base01 - Black
color1  = #d88568 # base08 - Red
color2  = #83a471 # base0B - Green
color3  = #b99353 # base0A - Yellow
color4  = #8eccdd # base0D - Blue
color5  = #b98eb2 # base0E - Magenta
color6  = #7c9cae # base0C - Cyan
color7  = #e8e8e8 # base06 - White
color8  = #1d414d # base02 - Bright Black
color9  = #d86868 # base09 - Bright Red
color10 = #222222 # base01 - Bright Green
color11 = #1d414d # base02 - Bright Yellow
color12 = #b8b8b8 # base04 - Bright Blue
color13 = #e8e8e8 # base06 - Bright Magenta
color14 = #8b6c37 # base0F - Bright Cyan
color15 = #f8f8f8 # base07 - Bright White
