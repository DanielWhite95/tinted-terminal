# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Windows NT Light (legacy)
# Scheme author: Fergus Collins (https://github.com/C-Fergus)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #808080
foreground_bold     = #808080
cursor              = #808080
cursor_foreground   = #ffffff
background          = #ffffff

color0  = #eaeaea # base01 - Black
color1  = #800000 # base08 - Red
color2  = #008000 # base0B - Green
color3  = #808000 # base0A - Yellow
color4  = #000080 # base0D - Blue
color5  = #800080 # base0E - Magenta
color6  = #008080 # base0C - Cyan
color7  = #404040 # base06 - White
color8  = #d5d5d5 # base02 - Bright Black
color9  = #ffff00 # base09 - Bright Red
color10 = #eaeaea # base01 - Bright Green
color11 = #d5d5d5 # base02 - Bright Yellow
color12 = #a0a0a0 # base04 - Bright Blue
color13 = #404040 # base06 - Bright Magenta
color14 = #00ff00 # base0F - Bright Cyan
color15 = #000000 # base07 - Bright White
