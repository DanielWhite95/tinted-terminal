# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Hardcore (legacy)
# Scheme author: Chris Caller
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #cdcdcd
foreground_bold     = #cdcdcd
cursor              = #cdcdcd
cursor_foreground   = #212121
background          = #212121

color0  = #303030 # base01 - Black
color1  = #f92672 # base08 - Red
color2  = #a6e22e # base0B - Green
color3  = #e6db74 # base0A - Yellow
color4  = #66d9ef # base0D - Blue
color5  = #9e6ffe # base0E - Magenta
color6  = #708387 # base0C - Cyan
color7  = #e5e5e5 # base06 - White
color8  = #353535 # base02 - Bright Black
color9  = #fd971f # base09 - Bright Red
color10 = #303030 # base01 - Bright Green
color11 = #353535 # base02 - Bright Yellow
color12 = #707070 # base04 - Bright Blue
color13 = #e5e5e5 # base06 - Bright Magenta
color14 = #e8b882 # base0F - Bright Cyan
color15 = #ffffff # base07 - Bright White
