background          = #00211f

color0  = #003a38 # base01 - Black
color1  = #ce7e8e # base08 - Red
color2  = #56c16f # base0B - Green
color3  = #bfac4e # base0A - Yellow
color4  = #88b0da # base0D - Blue
color5  = #b39be0 # base0E - Magenta
color6  = #62c0be # base0C - Cyan
color7  = #efefef # base06 - White
color8  = #005453 # base02 - Bright Black
color9  = #dca37c # base09 - Bright Red
color10 = #003a38 # base01 - Bright Green
color11 = #005453 # base02 - Bright Yellow
color12 = #c3c3c3 # base04 - Bright Blue
color13 = #efefef # base06 - Bright Magenta
color14 = #d89aba # base0F - Bright Cyan
color15 = #f5f5f5 # base07 - Bright White
