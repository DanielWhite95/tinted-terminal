# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Pasque (legacy)
# Scheme author: Gabriel Fontes (https://github.com/Misterio77)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #dedcdf
foreground_bold     = #dedcdf
cursor              = #dedcdf
cursor_foreground   = #271c3a
background          = #271c3a

color0  = #100323 # base01 - Black
color1  = #a92258 # base08 - Red
color2  = #c6914b # base0B - Green
color3  = #804ead # base0A - Yellow
color4  = #8e7dc6 # base0D - Blue
color5  = #953b9d # base0E - Magenta
color6  = #7263aa # base0C - Cyan
color7  = #edeaef # base06 - White
color8  = #3e2d5c # base02 - Bright Black
color9  = #918889 # base09 - Bright Red
color10 = #100323 # base01 - Bright Green
color11 = #3e2d5c # base02 - Bright Yellow
color12 = #bebcbf # base04 - Bright Blue
color13 = #edeaef # base06 - Bright Magenta
color14 = #59325c # base0F - Bright Cyan
color15 = #bbaadd # base07 - Bright White
