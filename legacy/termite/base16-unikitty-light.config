color7  = #4f4b51 # base06 - White
color8  = #c4c3c5 # base02 - Bright Black
color9  = #d65407 # base09 - Bright Red
color10 = #e1e1e2 # base01 - Bright Green
color11 = #c4c3c5 # base02 - Bright Yellow
color12 = #89878b # base04 - Bright Blue
color13 = #4f4b51 # base06 - Bright Magenta
color14 = #e013d0 # base0F - Bright Cyan
color15 = #322d34 # base07 - Bright White
