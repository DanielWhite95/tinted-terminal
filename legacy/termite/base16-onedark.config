# Termite Terminal Theme
# Scheme system: base16
# Scheme name: OneDark (legacy)
# Scheme author: Lalit Magant (http://github.com/tilal6991)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #abb2bf
foreground_bold     = #abb2bf
cursor              = #abb2bf
cursor_foreground   = #282c34
background          = #282c34

color0  = #353b45 # base01 - Black
color1  = #e06c75 # base08 - Red
color2  = #98c379 # base0B - Green
color3  = #e5c07b # base0A - Yellow
color4  = #61afef # base0D - Blue
color5  = #c678dd # base0E - Magenta
color6  = #56b6c2 # base0C - Cyan
color7  = #b6bdca # base06 - White
color8  = #3e4451 # base02 - Bright Black
color9  = #d19a66 # base09 - Bright Red
color10 = #353b45 # base01 - Bright Green
color11 = #3e4451 # base02 - Bright Yellow
color12 = #565c64 # base04 - Bright Blue
color13 = #b6bdca # base06 - Bright Magenta
color14 = #be5046 # base0F - Bright Cyan
color15 = #c8ccd4 # base07 - Bright White
