background          = #f7f9fb

color0  = #e5ebf1 # base01 - Black
color1  = #bf8b56 # base08 - Red
color2  = #56bf8b # base0B - Green
color3  = #8bbf56 # base0A - Yellow
color4  = #8b56bf # base0D - Blue
color5  = #bf568b # base0E - Magenta
color6  = #568bbf # base0C - Cyan
color7  = #223b54 # base06 - White
color8  = #cbd6e2 # base02 - Bright Black
color9  = #bfbf56 # base09 - Bright Red
color10 = #e5ebf1 # base01 - Bright Green
color11 = #cbd6e2 # base02 - Bright Yellow
color12 = #627e99 # base04 - Bright Blue
color13 = #223b54 # base06 - Bright Magenta
color14 = #bf5656 # base0F - Bright Cyan
color15 = #0b1c2c # base07 - Bright White
