color2  = #428b00 # base0B - Green
color3  = #a78300 # base0A - Yellow
color4  = #006dce # base0D - Blue
color5  = #825dc0 # base0E - Magenta
color6  = #00978a # base0C - Cyan
color7  = #3a4d53 # base06 - White
color8  = #d5cdb6 # base02 - Bright Black
color9  = #bc5819 # base09 - Bright Red
color10 = #ece3cc # base01 - Bright Green
color11 = #d5cdb6 # base02 - Bright Yellow
color12 = #909995 # base04 - Bright Blue
color13 = #3a4d53 # base06 - Bright Magenta
color14 = #c44392 # base0F - Bright Cyan
color15 = #3a4d53 # base07 - Bright White
