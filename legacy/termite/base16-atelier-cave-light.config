color4  = #576ddb # base0D - Blue
color5  = #955ae7 # base0E - Magenta
color6  = #398bc6 # base0C - Cyan
color7  = #26232a # base06 - White
color8  = #8b8792 # base02 - Bright Black
color9  = #aa573c # base09 - Bright Red
color10 = #e2dfe7 # base01 - Bright Green
color11 = #8b8792 # base02 - Bright Yellow
color12 = #655f6d # base04 - Bright Blue
color13 = #26232a # base06 - Bright Magenta
