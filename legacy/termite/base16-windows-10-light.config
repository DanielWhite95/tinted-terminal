color3  = #c19c00 # base0A - Yellow
color4  = #0037da # base0D - Blue
color5  = #881798 # base0E - Magenta
color6  = #3a96dd # base0C - Cyan
color7  = #414141 # base06 - White
color8  = #d9d9d9 # base02 - Bright Black
color9  = #f9f1a5 # base09 - Bright Red
color10 = #e5e5e5 # base01 - Bright Green
color11 = #d9d9d9 # base02 - Bright Yellow
color12 = #ababab # base04 - Bright Blue
color13 = #414141 # base06 - Bright Magenta
color14 = #16c60c # base0F - Bright Cyan
color15 = #0c0c0c # base07 - Bright White
