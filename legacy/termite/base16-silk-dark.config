color4  = #46bddd # base0D - Blue
color5  = #756b8a # base0E - Magenta
color6  = #3fb2b9 # base0C - Cyan
color7  = #cbf2f7 # base06 - White
color8  = #2a5054 # base02 - Bright Black
color9  = #fcab74 # base09 - Bright Red
color10 = #1d494e # base01 - Bright Green
color11 = #2a5054 # base02 - Bright Yellow
color12 = #9dc8cd # base04 - Bright Blue
color13 = #cbf2f7 # base06 - Bright Magenta
color14 = #9b647b # base0F - Bright Cyan
color15 = #d2faff # base07 - Bright White
