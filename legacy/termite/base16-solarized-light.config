# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Solarized Light (legacy)
# Scheme author: Ethan Schoonover (modified by aramisgithub)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #586e75
foreground_bold     = #586e75
cursor              = #586e75
cursor_foreground   = #fdf6e3
background          = #fdf6e3

color0  = #eee8d5 # base01 - Black
color1  = #dc322f # base08 - Red
color2  = #859900 # base0B - Green
color3  = #b58900 # base0A - Yellow
color4  = #268bd2 # base0D - Blue
color5  = #6c71c4 # base0E - Magenta
color6  = #2aa198 # base0C - Cyan
color7  = #073642 # base06 - White
color8  = #93a1a1 # base02 - Bright Black
color9  = #cb4b16 # base09 - Bright Red
color10 = #eee8d5 # base01 - Bright Green
color11 = #93a1a1 # base02 - Bright Yellow
color12 = #657b83 # base04 - Bright Blue
color13 = #073642 # base06 - Bright Magenta
color14 = #d33682 # base0F - Bright Cyan
color15 = #002b36 # base07 - Bright White
