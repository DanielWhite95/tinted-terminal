color2  = #59f176 # base0B - Green
color3  = #f3e877 # base0A - Yellow
color4  = #66b0ff # base0D - Blue
color5  = #f10596 # base0E - Magenta
color6  = #0ef0f0 # base0C - Cyan
color7  = #e0e0ff # base06 - White
color8  = #30305a # base02 - Bright Black
color9  = #fc8d28 # base09 - Bright Red
color10 = #20204a # base01 - Bright Green
color11 = #30305a # base02 - Bright Yellow
color12 = #b0b0da # base04 - Bright Blue
color13 = #e0e0ff # base06 - Bright Magenta
color14 = #f003ef # base0F - Bright Cyan
color15 = #f5f5ff # base07 - Bright White
