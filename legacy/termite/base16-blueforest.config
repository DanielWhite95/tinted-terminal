color3  = #91ccff # base0A - Yellow
color4  = #a2cff5 # base0D - Blue
color5  = #0099ff # base0E - Magenta
color6  = #80ff80 # base0C - Cyan
color7  = #91ccff # base06 - White
color8  = #273e5c # base02 - Bright Black
color9  = #ff8080 # base09 - Bright Red
color10 = #1e5c1e # base01 - Bright Green
color11 = #273e5c # base02 - Bright Yellow
color12 = #1e5c1e # base04 - Bright Blue
color13 = #91ccff # base06 - Bright Magenta
color14 = #e7e7e7 # base0F - Bright Cyan
color15 = #375780 # base07 - Bright White
