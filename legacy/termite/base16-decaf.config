# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Decaf (legacy)
# Scheme author: Alex Mirrington (https://github.com/alexmirrington)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #cccccc
foreground_bold     = #cccccc
cursor              = #cccccc
cursor_foreground   = #2d2d2d
background          = #2d2d2d

color0  = #393939 # base01 - Black
color1  = #ff7f7b # base08 - Red
color2  = #beda78 # base0B - Green
color3  = #ffd67c # base0A - Yellow
color4  = #90bee1 # base0D - Blue
color5  = #efb3f7 # base0E - Magenta
color6  = #bed6ff # base0C - Cyan
color7  = #e0e0e0 # base06 - White
color8  = #515151 # base02 - Bright Black
color9  = #ffbf70 # base09 - Bright Red
color10 = #393939 # base01 - Bright Green
color11 = #515151 # base02 - Bright Yellow
color12 = #b4b7b4 # base04 - Bright Blue
color13 = #e0e0e0 # base06 - Bright Magenta
color14 = #ff93b3 # base0F - Bright Cyan
color15 = #ffffff # base07 - Bright White
