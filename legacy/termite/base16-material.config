# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Material (legacy)
# Scheme author: Nate Peterson
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #eeffff
foreground_bold     = #eeffff
cursor              = #eeffff
cursor_foreground   = #263238
background          = #263238

color0  = #2e3c43 # base01 - Black
color1  = #f07178 # base08 - Red
color2  = #c3e88d # base0B - Green
color3  = #ffcb6b # base0A - Yellow
color4  = #82aaff # base0D - Blue
color5  = #c792ea # base0E - Magenta
color6  = #89ddff # base0C - Cyan
color7  = #eeffff # base06 - White
color8  = #314549 # base02 - Bright Black
color9  = #f78c6c # base09 - Bright Red
color10 = #2e3c43 # base01 - Bright Green
color11 = #314549 # base02 - Bright Yellow
color12 = #b2ccd6 # base04 - Bright Blue
color13 = #eeffff # base06 - Bright Magenta
color14 = #ff5370 # base0F - Bright Cyan
color15 = #ffffff # base07 - Bright White
