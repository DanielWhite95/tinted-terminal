color3  = #eebb00 # base0A - Yellow
color4  = #5b4a9f # base0D - Blue
color5  = #883e9f # base0E - Magenta
color6  = #3d94a5 # base0C - Cyan
color7  = #ffe178 # base06 - White
color8  = #242604 # base02 - Bright Black
color9  = #ee8800 # base09 - Bright Red
color10 = #1a1e01 # base01 - Bright Green
color11 = #242604 # base02 - Bright Yellow
color12 = #ffd129 # base04 - Bright Blue
color13 = #ffe178 # base06 - Bright Magenta
color14 = #a928b9 # base0F - Bright Cyan
color15 = #ffeba0 # base07 - Bright White
