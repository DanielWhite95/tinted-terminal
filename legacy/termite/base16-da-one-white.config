color7  = #000000 # base06 - White
color8  = #888888 # base02 - Bright Black
color9  = #ff9470 # base09 - Bright Red
color10 = #c8c8c8 # base01 - Bright Green
color11 = #888888 # base02 - Bright Yellow
color12 = #282828 # base04 - Bright Blue
color13 = #000000 # base06 - Bright Magenta
color14 = #b3684f # base0F - Bright Cyan
color15 = #000000 # base07 - Bright White
