# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Isotope (legacy)
# Scheme author: Jan T. Sott
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #d0d0d0
foreground_bold     = #d0d0d0
cursor              = #d0d0d0
cursor_foreground   = #000000
background          = #000000

color0  = #404040 # base01 - Black
color1  = #ff0000 # base08 - Red
color2  = #33ff00 # base0B - Green
color3  = #ff0099 # base0A - Yellow
color4  = #0066ff # base0D - Blue
color5  = #cc00ff # base0E - Magenta
color6  = #00ffff # base0C - Cyan
color7  = #e0e0e0 # base06 - White
color8  = #606060 # base02 - Bright Black
color9  = #ff9900 # base09 - Bright Red
color10 = #404040 # base01 - Bright Green
color11 = #606060 # base02 - Bright Yellow
color12 = #c0c0c0 # base04 - Bright Blue
color13 = #e0e0e0 # base06 - Bright Magenta
color14 = #3300ff # base0F - Bright Cyan
color15 = #ffffff # base07 - Bright White
