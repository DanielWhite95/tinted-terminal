color2  = #17ad98 # base0B - Green
color3  = #dc8a0e # base0A - Yellow
color4  = #796af5 # base0D - Blue
color5  = #bb60ea # base0E - Magenta
color6  = #149bda # base0C - Cyan
color7  = #d8d7da # base06 - White
color8  = #666369 # base02 - Bright Black
color9  = #d65407 # base09 - Bright Red
color10 = #4a464d # base01 - Bright Green
color11 = #666369 # base02 - Bright Yellow
color12 = #9f9da2 # base04 - Bright Blue
color13 = #d8d7da # base06 - Bright Magenta
color14 = #c720ca # base0F - Bright Cyan
color15 = #f5f4f7 # base07 - Bright White
