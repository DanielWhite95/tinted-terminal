color6  = #54fcfc # base0C - Cyan
color7  = #d2d2d2 # base06 - White
color8  = #383838 # base02 - Bright Black
color9  = #a85400 # base09 - Bright Red
color10 = #1c1c1c # base01 - Bright Green
color11 = #383838 # base02 - Bright Yellow
color12 = #7e7e7e # base04 - Bright Blue
color13 = #d2d2d2 # base06 - Bright Magenta
color14 = #00a800 # base0F - Bright Cyan
color15 = #fcfcfc # base07 - Bright White
