# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Circus (legacy)
# Scheme author: Stephan Boyer (https://github.com/stepchowfun) and Esther Wang (https://github.com/ewang12)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #a7a7a7
foreground_bold     = #a7a7a7
cursor              = #a7a7a7
cursor_foreground   = #191919
background          = #191919

color0  = #202020 # base01 - Black
color1  = #dc657d # base08 - Red
color2  = #84b97c # base0B - Green
color3  = #c3ba63 # base0A - Yellow
color4  = #639ee4 # base0D - Blue
color5  = #b888e2 # base0E - Magenta
color6  = #4bb1a7 # base0C - Cyan
color7  = #808080 # base06 - White
color8  = #303030 # base02 - Bright Black
color9  = #4bb1a7 # base09 - Bright Red
color10 = #202020 # base01 - Bright Green
color11 = #303030 # base02 - Bright Yellow
color12 = #505050 # base04 - Bright Blue
color13 = #808080 # base06 - Bright Magenta
color14 = #b888e2 # base0F - Bright Cyan
color15 = #ffffff # base07 - Bright White
