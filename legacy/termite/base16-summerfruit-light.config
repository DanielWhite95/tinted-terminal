# Termite Terminal Theme
# Scheme system: base16
# Scheme name: Summerfruit Light (legacy)
# Scheme author: Christopher Corley (http://christop.club/)
# Template author: Tinted Theming (https://github.com/tinted-theming/tinted-terminal)

[colors]
foreground          = #101010
foreground_bold     = #101010
cursor              = #101010
cursor_foreground   = #ffffff
background          = #ffffff

color0  = #e0e0e0 # base01 - Black
color1  = #ff0086 # base08 - Red
color2  = #00c918 # base0B - Green
color3  = #aba800 # base0A - Yellow
color4  = #3777e6 # base0D - Blue
color5  = #ad00a1 # base0E - Magenta
color6  = #1faaaa # base0C - Cyan
color7  = #151515 # base06 - White
color8  = #d0d0d0 # base02 - Bright Black
color9  = #fd8900 # base09 - Bright Red
color10 = #e0e0e0 # base01 - Bright Green
color11 = #d0d0d0 # base02 - Bright Yellow
color12 = #000000 # base04 - Bright Blue
color13 = #151515 # base06 - Bright Magenta
color14 = #cc6633 # base0F - Bright Cyan
color15 = #202020 # base07 - Bright White
