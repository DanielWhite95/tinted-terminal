color2  = #007400 # base0B - Green
color3  = #826b28 # base0A - Yellow
color4  = #0000ff # base0D - Blue
color5  = #a90d91 # base0E - Magenta
color6  = #318495 # base0C - Cyan
color7  = #404040 # base06 - White
color8  = #c0c0c0 # base02 - Bright Black
color9  = #eb8500 # base09 - Bright Red
color10 = #c0c0c0 # base01 - Bright Green
color11 = #c0c0c0 # base02 - Bright Yellow
color12 = #808080 # base04 - Bright Blue
color13 = #404040 # base06 - Bright Magenta
color14 = #826b28 # base0F - Bright Cyan
color15 = #5e5e5e # base07 - Bright White
