color2 0x008400 # base0B - Green
color3 0xaf8500 # base0A - Yellow
color4 0x0054cf # base0D - Blue
color5 0x6b40c3 # base0E - Magenta
color6 0x009a8a # base0C - Cyan
color7 0x282828 # base06 - White
color8 0xcdcdcd # base02 - Bright Black
color9 0xba3700 # base09 - Bright Red
color10 0xebebeb # base01 - Bright Green
color11 0xcdcdcd # base02 - Bright Yellow
color12 0x878787 # base04 - Bright Blue
color13 0x282828 # base06 - Bright Magenta
color14 0xdd0f9d # base0F - Bright Cyan
color15 0x282828 # base07 - Bright White
